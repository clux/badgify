{
  "name": "badgify",
  "description": "Rebuild readme markdown badges from package.json scripts",
  "author": "Eirik Albrigtsen <analsandblaster@gmail.com>",
  "version": "0.3.0",
  "stability": "unstable",
  "repository": {
    "type": "git",
    "url": "clux/badgify"
  },
  "keywords": [
    "svg",
    "shields",
    "badge",
    "generate"
  ],
  "main": "badgify.js",
  "bin": "badgify.js",
  "scripts": {
    "test": "nodeunit test/*.js --reporter=verbose",
    "coverage": "jscoverage badgify.js && BADGIFY_COV=1 nodeunit --reporter=lcov test",
    "coveralls": "npm run coverage | coveralls"
  },
  "dependencies": {},
  "devDependencies": {
    "nodeunit": "~0.9.0",
    "coveralls": "~2.11.1",
    "jscoverage": "~0.5.5"
  },
  "bugs": {
    "url": "http://github.com/clux/badgify/issues"
  },
  "license": "MIT"
}
